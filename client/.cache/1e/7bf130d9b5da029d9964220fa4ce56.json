ckground-position-y: -1800px;\n}\n\np {\n    padding: 10px;\n}\n\n.post, .itinerary, .about {\n    margin: 1em 1em;\n    padding: 2em;\n    background-color: #BFCACE;\n    overflow: auto;\n    height: 30%;\n}\n\n.content {\n    padding: 10px;\n}\n\n.about em {\n    font-size: 3em;\n    font-weight: bold;\n}\n\n.header-link {\n    text-decoration: none;\n}\n\n.header-title {\n    font-family: 'Lobster', cursive;\n    padding: 8px;\n    border: 1px solid white;\n}\n\n.navigation {\n    display: flex;\n}\n\n.navigation-itinerary, .navigation-about {\n    margin: 10px 10px;\n    padding: 8px;\n}\n\n.footer {\n    bottom: 0;\n    text-align: center;\n    width: 250px;\n    border: 1px solid black;\n    margin: 10px auto 20px auto;\n    flex-shrink: 0;\n    height: 45px;\n}\n\ntemplate {\n    display: none;\n}\n\n.gallery__master {\n    list-style: none;\n    font-size: 0.75em;\n    display: flex;\n    overflow-x: auto;\n    height: 20vh;\n}\n\n.gallery__thumb {\n    height: 20vh;\n    margin-right: 1em;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n}\n\n.gallery__thumb.selected .gallery__thumb-img-wrap {\n    border: 2px solid #333;\n}\n\n.gallery__thumb:hover {\n    cursor: pointer;\n}\n\n.gallery__thumb-img-wrap {\n    display: block;\n    border: 2px solid black;\n    width: 19vh;\n    height: 19vh;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n}\n\n.gallery__thumb-img {\n    max-width: 19vh;\n    max-height: 19vh;\n    vertical-align: middle;\n    text-align: center;\n}\n\n.gallery__detail-img-wrap {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    height: 78vh;\n}\n\n.gallery__detail-img {\n    margin-top: 2vh;\n    max-height: 78vh;\n    max-width: 98vw;\n}\n\n\n@media only screen and (max-width: 960px) {\n    .header {\n        display: flex;\n        align-items: center;\n        flex-direction: column;\n    }\n\n    .navigation-about {\n        text-align: center;\n    }\n\n    .fixed-footer {\n        position: static;\n    }\n\n    .container {\n        font-size: 18px;  \n    }\n    \n    .header {\n        padding: 5px;\n    }\n\n    #author {\n        width: 100%;\n        height: 100%;\n        margin: 0 auto;\n    }\n\n    .about {\n        display: block;\n        flex-direction: column;\n    }\n}\n\n@media only screen and (min-width: 401px) {\n    .header {\n        display: flex;\n        justify-content: space-between;\n    }\n\n    #author {\n        width: 30%;\n        height: 30%;\n        overflow: auto;\n        float: left; \n        padding-right: 20px;\n        padding-bottom: 20px;\n    }\n\n    .border-red {\n        border-right: 5px solid #ED4B3A;\n    }\n    \n    .border-yellow {\n        border-right: 5px solid #F5C023;\n    }\n    \n    .border-blue {\n        border-right: 5px solid #0380BC;\n    }\n\n    .container {\n        font-size: 18px;  \n    }\n\n    #author {\n        width: 30%;\n        height: 30%;\n        overflow: auto;\n        float: left; \n        padding-right: 20px;\n        padding-bottom: 20px;\n    }\n}"},"lineCount":null}},"error":null,"hash":"8d606805e9a03918c9d829a53e41f505","cacheData":{"env":{}}}